{
  "name": "@adview/react",
  "version": "1.0.14",
  "description": "AdView React",
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint . --max-warnings 0",
    "clean": "rm -rf .turbo node_modules dist"
  },
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.cjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.cjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./server": {
      "import": "./dist/server.cjs",
      "module": "./dist/server.js",
      "types": "./dist/server.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "keywords": [
    "adview",
    "ad widget",
    "react"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/geniusrabbit/adview.git"
  },
  "homepage": "https://github.com/geniusrabbit/adview/tree/main/packages/react#readme",
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0"
  },
  "dependencies": {
    "@adview/core": "^1.0.6"
  },
  "author": "AdView Team",
  "license": "Apache-2.0",
  "publishConfig": {
    "access": "public"
  }
}
